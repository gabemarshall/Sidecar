<!doctype html>
<html lang="en" ng-app="sidecar">
	<head>
  		<meta charset="utf-8">
  		<meta name="viewport" content="width=device-width, initial-scale=1">

  		<title>Sidecar</title>

  		<link rel="stylesheet" href="stylesheets/app.css"/>

  		<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
	</head>

	<body>
        <div class="off-canvas-contain left-align">
		<section class="off-canvas">
            <a href="#" class="oc-profile row toggle-trigger">
                <img src="img/avatar.png" class="oc-avatar left">
                <p class="oc-user left">Jon Bellah</p>
            </a>

            <ul class="oc-sublist no-bullets">
                <li><a href="#">Support</a></li>
                <li><a href="#">Settings</a></li>
                <li><a href="#">Log out</a></li>
            </ul>

            <ul class="oc-nav no-bullets" ng-controller="Nav">
                <li ng-class="getClass('/dashboard')" class="oc-nav--dashboard"><a href="/app">Dashboard</a></li>
                <li ng-class="getClass('/clients')" class="oc-nav--clients"><a href="#/clients">Clients</a></li>
                <li ng-class="getClass('/prospects')" class="oc-nav--prospects"><a href="#/prospects">Prospects</a></li>
                <li ng-class="getClass('/projects')" class="oc-nav--projects"><a href="#/projects">Projects</a></li>
                <li ng-class="getClass('/documents')" class="oc-nav--documents"><a href="#/documents">Documents</a></li>
                <li ng-class="getClass('/invoices')" class="oc-nav--invoices"><a href="#/invoices">Invoices</a></li>
                <li ng-class="getClass('/reports')" class="oc-nav--reports"><a href="#/reports">Reports</a></li>
            </ul>
        </section>

        <section class="off-canvas-content">
            <header class="app-header row">
                <div class="ah-left left">
                    <a class="ah-hamburger off-canvas-trigger hide-md left"></a>
                    <a href="/app" class="ah-logo"><img src="img/logo.svg" class="center"></a>
                </div>

                <div class="ah-right left">
                    <form class="ah-search left">
                        <input type="search" class="ah-search-input" placeholder="Search...">
                        <button type="submit" class="ah-search-btn"></button>
                    </form>

                    <div class="ah-profile-contain right">
                        <a class="ah-profile right">
                            <img src="img/avatar.png" class="ah-avatar left">
                            <p class="ah-user left">Jon Bellah</p>
                        </a>

                        <ul class="ah-sublist no-bullets">
                            <li class="ah-sublist--support"><a href="#">Support</a></li>
                            <li class="ah-sublist--settings"><a href="#">Settings</a></li>
                            <li class="ah-sublist--logout"><a href="#">Log out</a></li>
                        </ul>
                    </div>
                </div>
            </header>

            <%-body%>
        </section>
    </div>

        <script src="/lib/barekit/js/barekit.min.js"></script>

		<script src="lib/angular/angular.min.js"></script>
        <script src="/js/draganddrop.js"></script>
